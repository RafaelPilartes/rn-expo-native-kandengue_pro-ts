{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Release"
      },
      "prebuildCommand": "chmod +x eas-hooks/postPrebuild.sh",
      "postPrebuild": {
        "command": "./eas-hooks/postPrebuild.sh"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "buildConfiguration": "Release"
      },
      "prebuildCommand": "chmod +x eas-hooks/postPrebuild.sh",
      "postPrebuild": {
        "command": "./eas-hooks/postPrebuild.sh"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
